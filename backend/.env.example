# Rename to .env and fill with real values
PORT=5000
MONGO_URI=mongodb+srv://<user>:<password>@cluster0.mongodb.net/wedding?retryWrites=true&w=majority
FRONTEND_URL=http://localhost:5500
# Admin credentials (prefer using hashed password)
ADMIN_USERNAME=admin
# You can either set ADMIN_PASSWORD (plain) or ADMIN_PASSWORD_HASH (bcrypt hash)
ADMIN_PASSWORD=changeme
# Alternatively generate hash: node -e "console.log(require('bcrypt').hashSync('changeme', 10))"
# ADMIN_PASSWORD_HASH=$2b$10$...
ADMIN_JWT_SECRET=ReplaceWithStrongSecret
JWT_EXPIRES_IN=1h
